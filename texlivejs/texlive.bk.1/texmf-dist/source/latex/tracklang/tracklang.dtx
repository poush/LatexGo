%Parse localisation format.
%    \begin{macrocode}
\def\@tracklang@parse@locale#1{%
%    \end{macrocode}
%Initialise.
%    \begin{macrocode}
  \def\@TrackLangEnvLang{}%
  \def\@TrackLangEnvSubLang{}%
  \def\@TrackLangEnvFirstSubLang{}%
  \def\@TrackLangEnvTerritory{}%
