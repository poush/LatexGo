\def\@tracklang@ifalpha#1#2#3{%
  \ifx\relax#1\relax
%    \end{macrocode}
%First argument empty or \cs{relax}.
%    \begin{macrocode}
    #3%
  \else
   \ifnum\lccode`#1<`a\relax
     #3%
   \else
     \ifnum\lccode`#1>`z\relax
      #3%
     \else
%    \end{macrocode}
% Is alpha.
%    \begin{macrocode}
       #2%
     \fi
   \fi
  \fi
}
%    \end{macrocode}
